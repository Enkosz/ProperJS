  "description": "MVC Framework for Typescript based Web Applications",
  "main": "./lib/src/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev:server": "nodemon --watch './**/*.ts' --exec 'ts-node -r tsconfig-paths/register' properjs/app.ts",
    "start": "node ./lib/src/index.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Enkosz/ProperJS.git"
  },
  "author": "Simone Giannino",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Enkosz/ProperJS/issues"
  },
  "_moduleAliases": {
    "@core": "dist/rest/modules",
    "@services": "dist/services"
  },
  "homepage": "https://github.com/Enkosz/ProperJS#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.24",
    "@types/object-hash": "^2.2.1",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "express": "^4.17.3",
    "module-alias": "^2.2.2",
    "object-hash": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "tslog": "^3.3.3"
  }
}
